
@{
    Layout = null;
	ViewBag["Title"] = "Access Denied";
}

<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"]</title>
	<script src="~/lib/jquery/dist/jquery.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.js"></script>
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
	<link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
	<div class="container col-lg-6" style="margin-top:120px">
		<div class="card text-center">
			<div class="card-body">
				<strong class="alert-warning">You are not authorized to access this resource.</strong>

				<a class="btn btn-danger" asp-action="Index" asp-controller="Home">Go to HomePage</a>
			</div>
		</div>
	</div>
</body>
</html>